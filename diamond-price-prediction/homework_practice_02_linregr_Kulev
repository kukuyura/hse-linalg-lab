      "cell_type": "code",
      "execution_count": 9,
      "metadata": {
        "id": "nERVlRnnIEPC"
      },
      "outputs": [],
      "source": [
        "def error(f, X_local, y_local):\n",
        "    poly = np.poly1d(np.flip(f))\n",
        "    err = np.polyval(poly, X_local) - y_local\n",
        "    return np.sum(err * err) / X_local.shape[0]"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 10,
      "metadata": {
        "colab": {
